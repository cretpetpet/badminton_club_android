<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.badminton_club">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.example.asus.badminton_club.PleaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.asus.badminton_club.screen.main.MainActivity" />
        <activity android:name="com.example.asus.badminton_club.screen.authenication.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.asus.badminton_club.SettingMain" />
        <activity android:name="com.example.asus.badminton_club.screen.authenication.ForgotPassword" />
        <activity android:name="com.example.asus.badminton_club.ClubMainActivity" />
        <activity android:name="com.example.asus.badminton_club.screen.setting.SettingActivity" />
        <activity android:name="com.example.asus.badminton_club.screen.setting.SettingAccountEditActivity" />
        <activity android:name="com.example.asus.badminton_club.screen.authenication.SignUp" />
        <activity android:name="com.example.asus.badminton_club.screen.main.HomeMain" />
        <activity android:name="com.example.asus.badminton_club.CentralPageActivity" />

    </application>

</manifest>